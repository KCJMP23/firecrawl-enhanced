# WebClone Pro Environment Configuration

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/webclone_pro"
DIRECT_URL="postgresql://username:password@localhost:5432/webclone_pro"

# Redis Configuration (for caching and sessions)
REDIS_URL="redis://localhost:6379"

# AI Model Providers
OPENAI_API_KEY="sk-your-openai-api-key-here"
ANTHROPIC_API_KEY="your-anthropic-api-key-here"
GOOGLE_AI_API_KEY="your-google-ai-api-key-here"

# Vector Database (Qdrant)
QDRANT_URL="http://localhost:6333"
QDRANT_API_KEY="your-qdrant-api-key-here"

# WebHarvest Integration
WEBHARVEST_API_URL="http://localhost:8000"
WEBHARVEST_API_KEY="your-webharvest-api-key-here"

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-supabase-service-role-key"

# Stripe Configuration
STRIPE_PUBLIC_KEY="pk_test_your-stripe-public-key"
STRIPE_SECRET_KEY="sk_test_your-stripe-secret-key"
STRIPE_WEBHOOK_SECRET="whsec_your-webhook-secret"

# Next.js Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-here"

# LiveBlocks (Real-time Collaboration)
LIVEBLOCKS_SECRET_KEY="sk_test_your-liveblocks-secret"

# File Storage
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AWS_S3_BUCKET="your-s3-bucket-name"
AWS_REGION="us-east-1"

# Monitoring & Observability
SENTRY_DSN="https://your-sentry-dsn"
ANALYTICS_API_KEY="your-analytics-api-key"

# Deployment Configuration
VERCEL_URL="your-vercel-deployment-url"
RAILWAY_STATIC_URL="your-railway-deployment-url"

# Agent System Configuration
AGENT_MAX_EXECUTION_TIME="300000"  # 5 minutes in milliseconds
AGENT_MAX_CONCURRENT_TASKS="10"
AGENT_DEFAULT_MODEL_PROVIDER="anthropic"
AGENT_COST_OPTIMIZATION_ENABLED="true"

# Vector Database Settings
VECTOR_DIMENSION="384"  # For sentence-transformers/all-MiniLM-L6-v2
VECTOR_SIMILARITY_THRESHOLD="0.7"
VECTOR_MAX_RESULTS="50"

# WebHarvest Settings
SCRAPING_MAX_PAGES="20"
SCRAPING_RATE_LIMIT="1000"  # milliseconds between requests
SCRAPING_TIMEOUT="30000"    # 30 seconds
SCRAPING_USER_AGENT="WebClone-Pro/1.0"

# Security Settings
CSRF_SECRET="your-csrf-secret-here"
JWT_SECRET="your-jwt-secret-here"
ENCRYPTION_KEY="your-encryption-key-here"

# Feature Flags
FEATURE_AI_AGENTS="true"
FEATURE_VECTOR_SEARCH="true"
FEATURE_REAL_TIME_COLLABORATION="true"
FEATURE_ADVANCED_ANALYTICS="true"
FEATURE_DEPLOYMENT_AUTOMATION="true"

# Development Settings
NODE_ENV="development"
LOG_LEVEL="info"
DEBUG_MODE="false"